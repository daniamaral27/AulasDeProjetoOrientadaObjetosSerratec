package Aula02POO;

public class Conta {
	//atributos
	public int NumeroConta;
	public String Titular;
	public double Saldo;
	//metodos
	public void deposito(double valor) {
		saldo =+ valor;
	}

	public boolean Saque(double valor) {
		if (saldo < valor){
			return false;
		} else {
			return true;
		}
	}