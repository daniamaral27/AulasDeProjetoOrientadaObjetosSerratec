package exerciciosIndividuais;

public class testeCaneta {
//instanciar Ã© gerar objeto a partir da classe
	public static void main(String[] args) {
		Caneta c1 = new Caneta ();
		c1.cor = "azul";
		c1.ponta = 0.5f;
		c1.destampar();
		c1.rabiscar();
		c1.status();
		
		Caneta c2 = new Caneta();
		c2.cor = "vermelha";
		c2.ponta = 0.7f;
		c2.tampar();
		c2.rabiscar();
		c2.status();
	}
	
	
	

}
