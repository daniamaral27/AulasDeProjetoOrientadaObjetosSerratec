package exerciciosIndividuais;

public class Caneta {
	String modelo;
	String cor;
	float ponta;
	int carga;
	boolean tampada;
	
	void status() {
		System.out.println("Bic" + modelo + "Uma caneta azul" + cor + "A ponta é: " + ponta + "Está com carga: " + carga + "Está tampada");
		
	}
	
	void rabiscar () {
		System.out.println("modelo: " );
	}
		
	void tampar () {
		System.out.println("modelo: " );
	}
			
	void destampar () {
		System.out.println("modelo: " );
		}
}




