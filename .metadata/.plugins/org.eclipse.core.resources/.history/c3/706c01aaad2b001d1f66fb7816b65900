package exerciciosIndividuais;

public class Caneta {
	String modelo;
	String cor;
	float ponta;
	int carga;
	boolean tampada;
	
	void status() {
		System.out.println("Bic" + modelo + "\nUma caneta azul" + cor + "\nA ponta é: " + ponta + "\nEstá com carga: " + carga + "\nEstá tampada");
		
	}
	
	void rabiscar () {
		System.out.println("modelo: " );
	}
		
	void tampar () {
		this.tampada = true;
	}
			
	void destampar () {
		this.destampar = false;
		}
}




